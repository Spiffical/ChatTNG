# Database Settings
DATABASE_URL=postgresql+asyncpg://postgres:postgres123@db:5432/chattng

# Redis Settings
REDIS_URL=redis://redis:6379

# AWS Settings
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-west-2
S3_BUCKET=chattng-clips
CLOUDFRONT_DOMAIN=your_cloudfront_domain

# Pinecone Settings
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=gcp-starter
PINECONE_INDEX=chattng-dialogs

# OpenAI Settings
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=text-embedding-3-small

# API Settings
DEBUG=true
API_TITLE="ChatTNG API"
API_DESCRIPTION="Star Trek: TNG Dialog Chat API"
API_VERSION=1.0.0
PROJECT_ROOT=/app

# CORS Settings
CORS_ORIGINS=http://localhost:3000
CORS_ALLOW_CREDENTIALS=true

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# Session Settings
SESSION_COOKIE=chattng_session
SESSION_EXPIRE=86400  # 24 hours

# Share Settings
SHARE_EXPIRE_DAYS=7
SHARE_BASE_URL=http://localhost:3000

# Security
JWT_SECRET=replace_with_secure_secret
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=1440  # 24 hours

# Development Settings
PYTHONPATH=/app/backend
PORT=8080
FASTAPI_DEBUG=true
UVICORN_LOG_LEVEL=debug
UVICORN_RELOAD=true
UVICORN_WORKERS=1
LOG_LEVEL=debug 